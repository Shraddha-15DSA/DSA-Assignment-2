#include <iostream>

using namespace std;

int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;  

 
        for(int i=1;i<=n;i++){
for(int j=1;j<=n;j++){
    if(i==(n+1)/2||j==(n+1)/2||i==2&&j==4||i==4&&j==4){
        cout<<"*";
    }
        
        
    
    else
    cout<<" ";
}
cout<<"\n";
     

}
        

        
        
    return 0;
}
